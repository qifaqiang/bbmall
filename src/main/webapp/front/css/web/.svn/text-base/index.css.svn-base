/*左侧楼梯导航*/
.leftside{
        position: fixed;

        bottom:200px;
        z-index:1000;
        display: none;
        
}
.leftside ul li {
        width: 60px;
        height: 60px;
        margin-bottom: 8px;
        cursor:pointer;
}
	.gray { 
-webkit-filter: grayscale(100%); 
-moz-filter: grayscale(100%); 
-ms-filter: grayscale(100%); 
-o-filter: grayscale(100%); 
filter: grayscale(100%); 
filter: gray; 
} 

.moreWIndex{
	font-size:16px;
	color:#999999;
	line-height:30px;
	height:30px;
	
}
.leftside ul li div.imgLeftW{

	width:60px;
	height:60px;
}
.leftside ul li .divLeftH{
	font-size:16px;
	text-align:center;
	background:url('../../images/web/leftBgFix.png') no-repeat center;
	color:white;
	height:40px;
	width:40px;
	padding:10px;
	line-height:20px;
	
}


.leftside ul li div.imgLeftW{
        display: block;
}
.leftside ul li .divLeftH{
        display: none;
}

.leftside ul li.active div.imgLeftW{
        display: none;
}
.leftside ul li.active .divLeftH{
        display: block;
}
.leftside ul li:hover div.imgLeftW{
        display: none;
}
.leftside ul li:hover .divLeftH{
        display: block;
}

#indexlayer{
        position: relative;
}
#close{
        position: absolute;
        top:13px;
        right: 15px;
        width: 49px;
        height: 49px;
        background: black;
        opacity: 0;
        filter:alpha(opacity=0);
}
