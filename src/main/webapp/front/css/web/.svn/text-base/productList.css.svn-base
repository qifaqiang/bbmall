@charset "utf-8";

#allsort{
    display: none;
}
.firstAllW:hover #allsort{
    display: block;
}
.breadCrumbNav{
    height: 40px;
    line-height: 40px;
    color: #999;
}
.breadCrumbNav a{
    color: #999;
}
.leftSidePro{
    width: 250px;
    float: left;
}

.leftSideListTop{
    height: 35px;
    line-height: 35px;
    background: #f1f1f1;
    border: 1px solid #e5e5e5;
    font-size: 18px;
    text-align: center;
    border-top: 4px solid #8ab700;
    width: 248px;
    border-bottom: none;
}

.leftSideList{
    width: 208px;
    border: 1px solid #e5e5e5;
    padding:20px;
}
.hotProList li img{
    width: 206px;
    border: 1px solid #e5e5e5;
}
.leftSideWord{
    padding: 12px 0;
}
.leftSideWord p{
    line-height: 30px;
    height: 30px;
    font-size: 16px;
}
.Proprice{
    color: #e94925;
}
.hotProList a:hover{
    color: #333333;
}
.rightList{
    float: right;
    width: 900px;
}
.rightListTitle {
    background: #f1f1f1;
    font-size: 18px;
    height: 40px;
    line-height: 40px;
}
.rightListTitle p{
    padding-left: 10px;
}
.classifyList{
   padding: 10px 0 5px 0;
    border-bottom: 1px solid #e5e5e5;
    overflow: hidden;
    position: relative;
    height: 31px;
}
.ListTitleFront{
    margin-right: 10px;
    float: left;
    font-size: 14px;
}
.classifyDetails{
    float: left;
    width: 1000px;
}
.classifyListTotal{
    float: left;
}
.classifyListTotal span{

    display: inline-block;
    width: 70px;
    padding-top: 5px;
    padding-bottom: 5px;
    text-align: center;
}
.classifyListTotal .active{
    color: #ffffff;
    background: #8ab800;
}
.classifylonely{
    float: left;
    width: 900px;
}
.classifylonely span{
    display: block;
    float: left;
    /*width: 70px;*/
    padding:0 20px;
    padding-top: 5px;
    padding-bottom: 10px;
    text-align: center;

}
.classifylonely .active{
    color: #ffffff;
    background: #8ab800;
}
.ListTitleFront{
    display: inline-block;
    padding-top: 5px;
    padding-bottom: 5px;
}
.classifyListMore{
    position: absolute;
    border: 1px solid #e5e5e5;
    height: 28px;
    line-height: 28px;
    width: 62px;
    border-radius: 3px;
    text-align: center;
    right: 10px;
    top:8px;
}
.rightPro{
    margin-top: 25px;
}
.rightFilterCondtion{
    border-top: 4px solid #8ab800;
    height: 35px;
}
.rightFilterCondtionUl{
    height: 35px;
}
.rightFilterCondtionUl li{
    width: 110px;
    height: 35px;
    line-height: 31px;
    float: left;
    text-align: center;
    font-size: 18px;
    cursor: pointer;
    margin: 0 2px;
}
.rightFilterCondtionUl li.active, .rightFilterCondtionUl li:hover{
    background-image: url('../../images/web/proWhiteDown.png');
    background-color: #8ab800;
    color: #ffffff;
}
.rightFilterCondtionUl li{
    background-image: url('../../images/web/proGrayDown.png');
    background-repeat: no-repeat;
    background-position: 89px 13px;
}
.rightFilterCondtionUl li.active{

}
.rightFilterCondtionUl li.proCondtionDefault{
    background-image: none;
}
.rightFilterCondtionUl li.active.up{
    background-image: url('../../images/web/proWhiteUp.png');
}
.proBottom{
    margin:0px 0 22px 0;
    overflow: hidden;
}
.proBottom span{
    width: 90px;
    height: 25px;
    font-size: 16px;
}

.rightProList{
    overflow: hidden;
}
.rightProList li{
    float: left;
    margin-right: 22px;
}
.proBottom .proCollectionBtn{
    background-image: url("../../images/web/listUnco.png");
    background-position: 15px center;
    background-repeat: no-repeat;
    border: 1px solid #e5e5e5;
    color: #999999;
    float: left;
    padding-left: 39px;
    width: 51px;
}
.proBottom .proCollectionBtn.active{
    background-image: url("../../images/web/alreadyCollS.png");
    background-position: 12px center;
    color: #8ab800;
    padding-left: 34px;
    width: 56px;
}
.proShoppingCartBtn{
    float: right;
    border: 1px solid #e5e5e5;
    text-align: center;
    color: #999999;
}
.proShoppingCartBtn.active{

    background-color: #8ab800;
    color: #ffffff;

}
